10 REM TESTHTTPWEBSOCK TO UCHINPUT
20 REM I TO UCHSTR EAMIS&3
90 PINM LED ,1
100 COLOR 4
110 FRECT 120,140,200,200
120 @X=100%8+2: @Y=100%20+2: PUT &2,"ON"
130 COLOR 0
140 FRECT 320,140,200,200
150 @X=100%8+2: @Y=100%20+2: PUT &2,"OFF"
160 FOR I=0 TO 10
200 INPUT &3,E : REM &3 is mouse/touch stream, E is event or button
210 IF E=0 THEN  GOTO 200
220 INPUT &3,X,Y : REM X,Y
230 REM TOUCH: 128: START,129,MOVE, 130 STOP 132,  MOUSE >128 BUTTON bit 0,1,2 
240 IF E>128 THEN  GOTO 200
250 IF(X>120) AND(X<320) AND(Y>140) AND(Y<340) THEN DWRITE LED ,1: PRINT "LED ON!"
260 IF(X>320) AND(X<520) AND(Y>140) AND(Y<340) THEN DWRITE LED ,0: PRINT "LED OFF!"
300 GOSUB 600
310 FCIRCLE X,Y,30
320 NEXT
330 END
600 REM R AND OMCOLOR
620 R=INT(RND(1)*255)
630 G=INT(RND(1)*255)
640 B=INT(RND(1)*255)
650 COLOR R,G,B
660 RETURN

